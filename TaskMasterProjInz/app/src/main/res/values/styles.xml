<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">@color/bardzo_jasniutki</item>
        <item name="colorPrimaryDark">@color/ciemniejszy</item>
        <item name="colorAccent">@color/ciemniejszy</item>
        <item name="android:colorBackground">@color/bialy</item>

    </style>

    <style name="AppDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">@color/jasny</item>
        <item name="colorPrimaryDark">@color/ciemniejszy</item>
        <item name="colorAccent">@color/ciemniejszy</item>

    </style>



    <color name="my_text">#0099CC</color>
    <color name="jasny">#f4f4f4</color>
    <color name="ciemniejszy">#0099CC</color>
    <color name="bardzo_jasniutki">#e9e9e9</color>
    <color name="bialy">#fafafa</color>



</resources>
